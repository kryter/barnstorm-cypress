{
  "name": "@kryter/barnstorm-cypress",
  "version": "2.0.0",
  "description": "Cypress specific implementation for end to end tests organized with Barnstorm",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**' && npx prettier --check .",
    "lint:fix": "eslint --fix 'src/**'",
    "format": "prettier --write 'src/**'",
    "precommit": "tsc && eslint --fix 'src/**' && prettier --write 'src/**'"
  },
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kryter/barnstorm-cypress.git"
  },
  "keywords": [
    "automation",
    "barnstorm",
    "browser",
    "cypress",
    "cypress.io",
    "e2e",
    "end-to-end",
    "integration",
    "test",
    "testing"
  ],
  "author": "Kori Ryter <kryter@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kryter/barnstorm-cypress/issues"
  },
  "homepage": "https://github.com/kryter/barnstorm-cypress#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@kryter/barnstorm": "^1.0.5",
    "cypress": "^8.3.1"
  }
}
